CREATE TABLE finance.bond (   code varchar(6) NOT NULL default '' ,   name varchar(20) NOT NULL default '' ,   facePrice real default NULL ,   netPrice real default NULL ,   fullPrice real default NULL ,   accruedInterest real default NULL,   turnVolume real default NULL ,   inpaydays integer default NULL ,   couponRate real default NULL ,   bondCredit varchar(6) NOT NULL default '' ,   mainCredit varchar(20) NOT NULL default '' ,   assukind varchar(60) NOT NULL default '' ,   dueDate date default NULL ,   amount real default NULL ,   SRC_DT date default NULL ,   ETL_DT date default NULL  ) ;  Comment on Table  finance.bond      is '債券' ;Comment on Column finance.bond.code is  '債券代碼' ;Comment on Column finance.bond.name  is  '債券名称' ;Comment on Column finance.bond.facePrice is   '面值(元)' ;Comment on Column finance.bond.netPrice  is  '净价(元)' ;Comment on Column finance.bond.fullPrice  is  '全价(元)' ;Comment on Column finance.bond.accruedInterest  is  '应计利息' ;Comment on Column finance.bond.turnVolume  is  '成交额(万元)' ;Comment on Column finance.bond.inpaydays  is  '距付息天数' ;Comment on Column finance.bond.couponRate  is  '票面利率(%) ' ;Comment on Column finance.bond.bondCredit is  '债券信用' ;Comment on Column finance.bond.mainCredit  is  '主体信用' ;Comment on Column finance.bond.assukind  is  '担保方式' ;Comment on Column finance.bond.dueDate is   '到期日' ;Comment on Column finance.bond.amount  is  '规模 (亿) ' ;Comment on Column finance.bond.SRC_DT is   '统计日期' ;Comment on Column finance.bond.ETL_DT is   '系统处理日期' ;  